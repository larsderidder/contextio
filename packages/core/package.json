  "description": "Shared types, routing, and header utilities for the @context ecosystem. Zero dependencies.",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist/"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/larsderidder/contextio.git",
    "directory": "packages/core"
  },
  "homepage": "https://github.com/larsderidder/contextio#readme",
  "bugs": "https://github.com/larsderidder/contextio/issues",
  "keywords": [
    "llm",
    "proxy",
    "api",
    "context",
    "http"
