{
  "name": "@contextio/cli",
  "version": "0.0.1",
  "description": "CLI for contextio: LLM API proxy with redaction and logging",
  "license": "MIT",
  "type": "module",
  "bin": {
    "ctxio": "dist/main.js",
    "contextio": "dist/main.js"
  },
  "files": [
    "dist/",
    "mitm_addon.py"
  ],
  "scripts": {
    "build": "tsc",
    "build:test": "tsc -p tsconfig.test.json",
    "test": "pnpm run build:test && node --test 'dist-test/test/**/*.test.js'",
    "dev": "tsc --watch",
    "clean": "rm -rf dist dist-test",
    "prepublishOnly": "pnpm run build"
  },
  "dependencies": {
    "@contextio/core": "workspace:*",
    "@contextio/proxy": "workspace:*",
    "@contextio/redact": "workspace:*",
    "@contextio/logger": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^22",
    "typescript": "^5.7"
  }
}
